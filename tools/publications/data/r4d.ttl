    <dc:title>Young Lives Policy Brief 1. The Social Impacts of Trade Liberalisation: how can childhood poverty be reduced?</dc:title>
    <dc:date>2005-01-01T00:00:00</dc:date>
    <j.0:fundingPhase rdf:resource="http://data.younglives.org.uk/data/younglivesStudyStructure/FundingPhaseTwo"/>
    <j.1:isPartOf rdf:resource="http://www.dfid.gov.uk/r4d/SearchResearchDatabase.asp?projectid=3967"/>
