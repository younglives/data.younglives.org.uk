    <dc:title>Understanding the Impacts of Crisis on Children in Developing Countries</dc:title>
    <j.1:isPartOf rdf:resource="http://www.dfid.gov.uk/r4d/SearchResearchDatabase.asp?projectid=60637"/>
    <dc:format>application/pdf</dc:format>
