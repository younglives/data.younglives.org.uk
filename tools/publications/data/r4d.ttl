    <j.1:hasPart rdf:resource="http://www.dfid.gov.uk/r4d/PDF/Outputs/R8358d.pdf"/>
    <dc:identifier>http://www.dfid.gov.uk/r4d/SearchResearchDatabase.asp?outputid=170703</dc:identifier>
