    <dc:title>Working Paper 47. Starting school: who is prepared? Young Lives' research on children's transition to first grade in Peru.</dc:title>
    <dc:subject rdf:resource="http://dbpedia.org/resource/Longitudinal_Studies"/>
    <dc:creator>P. Ames, V. Rojas and T. Portugal</dc:creator>
    <j.1:hasPart rdf:resource="http://www.dfid.gov.uk/r4d/PDF/Outputs/YoungLives/YL_WP47_Ames.pdf"/>
    <j.1:hasPart rdf:resource="http://www.dfid.gov.uk/r4d/PDF/Outputs/YoungLives/WP47_Summary.pdf"/>
