    <dc:title>Schooling as Lived and Told: Contrasting Impacts of Education Policies for Ethnic Minority Children in Vietnam seen from Young Lives Surveys</dc:title>
    <dc:subject rdf:resource="http://dbpedia.org/resource/Longitudinal_Studies"/>
    <j.1:license rdf:resource="http://reference.data.gov.uk/id/open-government-licence"/>
