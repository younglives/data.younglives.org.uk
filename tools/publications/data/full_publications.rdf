   <dc:title>Working Paper 32. Exploring Linkages between Maternal Social Capital and Children's Nutritional Status in Andhra Pradesh.</dc:title>
   <dc:coverage rdf:resource="http://www.fao.org/countryprofiles/geoinfo/geopolitical/resource/India"/>
