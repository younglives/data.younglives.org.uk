   <dc:title>Working Paper 13. Economic Shocks and Changes in School Attendance Levels and Education Expenditure in Peru.</dc:title>
   <dc:subject rdf:resource="http://dbpedia.org/resource/Young_Lives"/>
   <dc:identifier>http://www.dfid.gov.uk/r4d/SearchResearchDatabase.asp?outputid=173459</dc:identifier>
   <dc:type>text</dc:type>
   <dc:subject rdf:resource="http://dbpedia.org/resource/Longitudinal_Studies"/>
