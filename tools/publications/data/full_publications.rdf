   <dc:title><![CDATA[Technical Note 15. Psychometric characteristics of
cognitive development and achievement instruments
in Round 2 of Young Lives.]]></dc:title>
   <dc:subject rdf:resource="http://dbpedia.org/resource/Longitudinal_Studies"/>
   <j.1:hasPart rdf:resource="http://www.dfid.gov.uk/r4d/PDF/Outputs/YoungLives/YL-TN15-Cueto.pdf"/>
   <j.1:license rdf:resource="http://reference.data.gov.uk/id/open-government-licence"/>
