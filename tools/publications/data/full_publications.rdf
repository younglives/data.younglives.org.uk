   <dc:title>Technical Note No. 10. Children's Perspectives on their Young Lives: Report on methods for sub-studies.</dc:title>
   <dc:date>2008-01-01T00:00:00</dc:date>
   <j.1:hasPart rdf:resource="http://www.dfid.gov.uk/r4d/PDF/Outputs/YoungLives/TN10_Methods_Peru_Johnston.pdf"/>
